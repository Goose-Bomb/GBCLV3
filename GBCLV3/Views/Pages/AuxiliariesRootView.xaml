<ContentControl x:Class="GBCLV3.Views.AuxiliariesRootView"
                xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                xmlns:s="https://github.com/canton7/Stylet"
                Style="{StaticResource BlurInPageStyle}">

    <TabControl>
        <TabItem Header="&#xEA86;" Tag="{DynamicResource Mods}">
            <ContentControl s:View.Model="{Binding ModVM}" />
        </TabItem>

        <TabItem Header="&#xE7B8;" Tag="{DynamicResource Resourcepacks}">
            <ContentControl s:View.Model="{Binding ResourcePackVM}" />
        </TabItem>

        <!--<TabItem Header="&#xEA35;" Tag="{DynamicResource Saves}">
            <ContentControl s:View.Model="{Binding SaveVM}"/>
        </TabItem>-->

        <!--TabItem Header="&#xE790;" Tag="{DynamicResource Skins}">
            <ContentControl s:View.Model="{Binding SkinVM}"/>
        </TabItem-->
    </TabControl>

</ContentControl>